[app]
title = FingerCapture
package.name = fingercapture
package.domain = org.roshan
source.dir = .
source.include_exts = py,png,jpg,md
requirements = python3,kivy
orientation = portrait
fullscreen = 1
android.permissions = WRITE_EXTERNAL_STORAGE,READ_EXTERNAL_STORAGE

[buildozer]
log_level = 2
warn_on_root = 1
